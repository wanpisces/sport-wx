
.temp-record {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  width: 100%;
  height: 80rpx;
  background: #efeff4;
  color: #353535;
}

.temp-record-ed {
  color: #999;
}

.temp-record view {
  font-size: 24rpx;
  text-align: center;
  flex-grow: 0;
  flex-shrink: 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.temp-record image {
  width: 38rpx;
  height: 38rpx;
  border-radius: 19rpx;
  background: #999;
  margin-right: 10rpx;
}
