<import src="../../../../we-cropper/we-cropper.wxml" />
<navigationBar class='navigationBar' is-back="{{true}}" title-name="封面剪裁" img-url="/img/bg.png" status-bar-height="{{statusBarHeight}}" />
<view class="cropper-wrapper">
  <view class="cropper-view">
    <template is="we-cropper" data="{{...cropperOpt}}" />
  </view>

  <view class="cropper-buttons">
    <view class="upload" bindtap="uploadTap">
      重新选择
    </view>
    <view class="getCropperImage" bindtap="getCropperImage">
      确定
    </view>
  </view>
</view>