/* page/tabBar/index/index.wxss */
@import "/my-template/shuoshuo/shuoshuo.wxss";
@import "/my-template/team/team.wxss";
@import "/my-template/team3/team3.wxss";
@import "/my-template/load/listTemplate.wxss";

.pages {
  position: relative;
  width: 100%;
  height: 100%;
  display: flex;
}

.navigationBar {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  opacity: 1;
  z-index: 1000;
}

.v1 {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.bg {
  margin-top: 0rpx;
  width: 750rpx;
  height: 320px;
}

.data_view {
  position: absolute;
  width: 100%;
  top: 65px;
  left: 0;
  z-index: 2;
  display: flex;
  flex-flow: column;
  justify-content: flex-start;
  align-items: center;
}

.headPortrait {
  margin-top: 45rpx;
  height: 130rpx;
  width: 130rpx;
  border: 2px;
  background: #fff;
  border-radius: 65rpx;
  overflow: hidden;
}

.ranks1 {
  width: 100%;
  /* display: flex; *//* flex-flow: row nowrap; *//* align-items: center; */
}

.found {
  background: #ffd100;
  line-height: 86rpx;
  color: #3b3a43;
  font-size: 34rpx;
  padding: 0 45rpx;
  font-weight: bold;
  border-radius: 43rpx;
  margin: 50rpx 0 0 50rpx;
}

.found::after {
  border: 0;
}

.join {
  line-height: 86rpx;
  color: #ffd100;
  font-size: 32rpx;
  margin: 50rpx 0 0 0;
  padding: 0 45rpx;
  border-radius: 43rpx;
  border: 1px solid #ffd100;
  background: rgba(0, 0, 0, 0);
}

.join::after {
  border: 0;
}

.ssList {
  /* padding-top: 20rpx;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx; *//* padding-top: 40rpx; */
  border-top-left-radius: 25rpx;
  border-top-right-radius: 25rpx;
  margin-top: 80rpx;
  width: 100%;
  background: #fff;
}

.ranks2 {
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  height: auto;
}

.noDataImg {
  height: 277rpx;
  width: 277rpx;
}

.ranks2 .tip1, .ranks2 .tip2 {
  font-size: 30rpx;
  color: #999;
  padding: 10rpx;
}

.ranks2 .tip1, .ranks2 .tip2 {
  font-size: 30rpx;
  color: #999;
  padding: 10rpx;
}

.ranks2 .fondBnt {
  font-size: 30rpx;
  color: #ffd100;
  font-weight: bold;
  line-height: 50rpx;
  padding: 0 30rpx;
  border: 1rpx solid #ffd100;
  border-radius: 8rpx;
  background: #fff;
  margin-top: 30rpx;
}

.ranks2 .fondBnt::after {
  border: 0;
}

.userTeam {
  display: flex;
  flex-flow: column;
  /* border-bottom: 20rpx solid #eee; */
}

.userTeam .addTeam {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  padding: 10rpx 30rpx;
  margin: 0;
  line-height: 1;
  background: #fff;
  width: auto;
  height: auto;
  border-radius: 0;
}

.userTeam .addTeam::after {
  border: 0;
}

.userTeam .addTeam .addTeamImg {
  margin-top: -5rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: #ffd100;
}

.userTeam .addTeam .addTeamName {
  font-size: 30rpx;
  color: #353535;
  font-weight: bold;
  margin-left: 8rpx;
}

.userTeam .lookTeams {
  font-size: 26rpx;
  color: #999;
  line-height: 88rpx;
  background: #fff;
  border-radius: 0;
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
}

.userTeam .lookTeams::after {
  border: 0;
}

.lookTeamsImg {
  width: 36rpx;
  height: 36rpx;
  transform: rotate(90deg);
  margin-left: 8rpx;
}

.team-dynamics {
  display: flex;
  justify-content: space-between;
  line-height: 90rpx;
  font-size: 32rpx;
  color: #353535;
  padding: 0 32rpx;
  background: #fff;
}

/* 样式修改 */

.myteamList {
  display: flex;
  justify-content: space-between;
  font-size: 34rpx;
  color: #353535;
  margin: 32rpx;
}

.team-button {
  display: flex;
  justify-content: center;
}

.team-contain {
  width: 70%;
  display: flex;
  justify-content: space-around;
  padding-bottom: 45rpx;
}

.team-space {
  font-size: 26rpx;
  color: #999;
  text-align: center;
}

/* 热搜推荐 */

.team-module {
  position: relative;
  margin-bottom: 23rpx;
  padding: 0 30rpx;
}

.hot-contain {
  width: 100%;
}

.checkMore {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #999;
}

.checkMore>image {
  width: 32rpx;
  height: 32rpx;
}

.team-module {
  position: relative;
  margin-bottom: 23rpx;
  padding: 0 30rpx;
}

.txt {
  font-size: 24rpx;
  color: #353535;
  width: 160rpx;
  height: 50rpx;
  border: 1rpx solid #fed309;
  border-radius: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.txt>text {
  display: block;
  font-size: 30rpx;
  color: #ffd100;
  line-height: 50rpx;
  margin-right: 10rpx;
}
