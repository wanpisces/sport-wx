/* page/tabBar/movement-circle/movement-circle.wxss */
@import "/my-template/shuoshuo/shuoshuo.wxss";
@import "/my-template/load/listTemplate.wxss";
page{
  display: flex;
  flex-flow: column;
}
.navBar {
  border-bottom: 1px solid #f2f2f2;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1000;
  background: #fff;
}

.title {
  height: 45px;
  line-height: 45px;
  font-size: 34rpx;
  margin-left: 20px;
  color: 000;
  font-weight: bold;
}

.find-bottom {
  height: 98rpx;
  width: 100%;
}

.bottom-view {
  position: fixed;
  bottom: 30px;
  right: 20px;
  display: flex;
  flex-flow: column;
  align-items: center;
  z-index: 2999;
}

.bottom-view image {
  height: 56px;
  width: 56px;
  z-index: 100;
}
