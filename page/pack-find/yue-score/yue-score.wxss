}

.yue-edit {
  width: 273rpx;
  height: 90rpx;
  background: linear-gradient(90deg, rgba(255, 209, 0, 1) 0%, rgba(255, 174, 0, 1) 100%);
  box-shadow: 0px 3px 16px rgba(179, 33, 36, 0.24);
  border-radius: 45rpx;
  font-size: 35rpx;
  color: #222;
  line-height: 90rpx;
  position: absolute;
  bottom: 62rpx;
  left: 50%;
  transform: translateX(-50%);
