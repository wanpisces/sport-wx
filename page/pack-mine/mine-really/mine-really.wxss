.navigationBar-bg {
  width: 100%;
  height: 527rpx;
}

.navigationBar-bg image {
  width: 100%;
  height: 527rpx;
}

.really-contain {
  width: 100%;
  background-color: #f2f2f2;
  border-radius: 30px 30px 0px 0px;
  position: absolute;
  top: 260rpx;
  left: 0;
}

.really-title {
  width: 100%;
  background-color: #fff;
  border-radius: 30rpx 30rpx 0px 0px;
}

.really-title-h {
  height: 100rpx;
  font-size: 32rpx;
  color: #1e1e1e;
  padding-top: 33rpx;
  margin-left: 19rpx;
}

.really-title-h>.really-icon {
  font-size: 32rpx;
  color: #ffb600;
}

.really-title-h>.really-person {
  font-size: 22rpx;
  color: #6e6e6e;
}

.really-title-input {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  margin-left: 37rpx;
  border-bottom: 1px solid #f2f2f2;
}

.really-title-input:last-child {
  border-bottom: none;
}

.really-title-input>text:first-child {
  width: 140rpx;
  font-size: 30rpx;
  color: rgba(30, 30, 30, 1);
}

.really-title-input>text:last-child {
  font-size: 30rpx;
  color: #1e1e1e;
  padding-left: 30rpx;
}

input {
  font-size: 30rpx;
  color: #1e1e1e;
  margin-left: 35rpx;
}

.really-idPhone {
  width: 100%;
  background-color: #fff;
  margin-top: 20rpx;
}

.really-idPhone>view:nth-child(2) {
  width: 100%;
  display: flex;
  justify-content: space-around;
  padding-bottom: 40rpx;
}

.really-idPhone-image {
  width: 185rpx;
  height: 210rpx;
  background-color: #f2f6f9;
  border: 1px #c8cac9 dashed;
}

.really-idPhone-image:first-child {
  border: none;
}

.really-idPhone-image:nth-child(2) {
  height: 210rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.really-idPhone-image text {
  display: block;
  font-size: 18rpx;
  color: rgba(110, 110, 110, 1);
  text-align: center;
}

.idPhone-image image {
  display: block;
  width: 57rpx;
  height: 49rpx;
  margin: 0 auto;
}

.idPhone-image-image image {
  width: 185rpx;
  height: 210rpx;
}

.idPhone-image .idPhone-image-big {
  width: 187rpx;
  height: 211rpx;
}

.idPhone-image>view {
  width: 100%;
  height: 50%;
}

.really-idPhone-image>.idPhone-image {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.idPhone-image-if {
  position: relative;
}

.idPhone-image-if image {
  display: block;
  width: 185rpx;
  height: 210rpx;
  margin: 0 auto;
}

.idPhone-image-if>view {
  width: 185rpx;
  height: 38rpx;
  background: rgba(28, 28, 28, 0.5);
  position: absolute;
  bottom: 0;
  left: 0;
}

.idPhone-image-if>view>text {
  display: block;
  font-size: 24rpx;
  line-height: 38rpx;
  text-align: center;
  color: #fff;
}

.really-auditState image {
  display: block;
  width: 269rpx;
  height: 269rpx;
  margin: auto;
}

.really-text {
  width: 100%;
  background-color: #fff;
  margin-top: 20rpx;
  padding-bottom: 50rpx;
}

.really-text>view:nth-child(2) {
  width: 100%;
  display: flex;
  justify-content: center;
}

.really-text-image {
  width: 325rpx;
  height: 210rpx;
  background-color: #f2f6f9;
  border: 1px #c8cac9 dashed;
  display: flex;
  justify-content: center;
  align-items: center;
}

.really-text-image:first-child {
  margin-right: 20rpx;
}

.really-img  image {
  display: block;
  width: 75rpx;
  height: 64rpx;
  margin: 0 auto;
}

.really-text-image text {
  display: block;
  font-size: 22rpx;
  color: #6e6e6e;
}

.really-submit {
  width: 450rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 35rpx;
  color: #222;
  background: linear-gradient(0deg, rgba(255, 209, 0, 1), rgba(255, 174, 0, 1));
  border-radius: 45rpx;
  box-shadow: 0px 0px 16rpx rgba(179, 33, 36, 0.24);
  margin: auto;
  margin-top: 105rpx;
}

.really-reSubmit {
  width: 450rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 35rpx;
  color: #222;
  background: linear-gradient(0deg, rgba(255, 209, 0, 1), rgba(255, 174, 0, 1));
  border-radius: 45rpx;
  box-shadow: 0px 0px 16rpx rgba(179, 33, 36, 0.24);
  margin: auto;
}

.really-end .really-submitThree {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 36rpx 0 78rpx 0;
}

.really-submitThree>view {
  font-size: 30rpx;
  font-family: HiraginoSansGB-W3;
  color: rgba(63, 61, 61, 1);
  line-height: 60rpx;
}

.really-submitThree>text {
  display: inline-block;
  width: 18rpx;
  height: 18rpx;
  line-height: 18rpx;
  background: rgba(255, 209, 0, 1);
  border-radius: 50%;
  margin-right: 13rpx;
}

.really-phone {
  position: relative;
}

.really-phone>.really-phone-img {
  width: 325rpx;
  height: 210rpx;
}

.really-phone>.really-phone-icon {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  position: absolute;
  right: 0;
  bottom: 0;
}
